DOM.render(\n  <React.StrictMode>\n    <App />\n  </React.StrictMode>,\n  document.getElementById('root')\n);",["62","63"],"/home/brandon/the-odin-project/javascript-path/projects/shopping-cart/src/App.js",["64"],"import React, { useState } from 'react';\nimport Routes from './Routes';\n\nfunction App() {\n  const [cartArr, setCartArr] = useState([]);\n\n  const addToCart = (cartItem, amount) => {\n    if (cartArr.some(item => item.title === cartItem.title)) {\n      let newArr = cartArr;\n      newArr[newArr.findIndex(item => item.title === cartItem.title)].qty += parseInt(amount);\n      setCartArr(newArr);\n    } else {\n      cartItem.qty = parseInt(amount);\n      setCartArr((prevArr) => [...prevArr, cartItem]);\n    }\n  };\n\n  const editQtyInCart = (cartItem, amount) => {\n    let newArr = cartArr;\n    newArr[newArr.findIndex(item => item.title === cartItem.title)].qty = parseInt(amount);\n    setCartArr(newArr);\n  }\n\n  const importAllImages = (r) => {\n    let images = {}; \n    r.keys().map((item, index) => { images[item.replace('./', '')] = r(item); });\n    return images;\n  }\n\n  return (\n    <div className=\"app\">\n      <Routes cartArr={cartArr} addToCart={addToCart} importAllImages={importAllImages} editQtyInCart={editQtyInCart} />\n    </div>\n  );\n}\n\nexport default App;\n","/home/brandon/the-odin-project/javascript-path/projects/shopping-cart/src/Routes.js",["65","66"],"import React, { useEffect } from 'react';\nimport { BrowserRouter, Switch, Route, Link, Redirect } from 'react-router-dom';\nimport { Eyeglasses, Cart3 } from 'react-bootstrap-icons';\nimport App from './App';\nimport Homepage from './Homepage';\nimport ShoppingPage from './ShoppingPage';\nimport CartPage from './CartPage';\nimport ItemPage from './ItemPage';\n\nconst Routes = (props) => {\n  const { cartArr, addToCart, importAllImages, editQtyInCart } = props;\n\n  useEffect(() => {\n    console.log('Render');\n  });\n\n  return (\n    <BrowserRouter>\n      <div>\n      