<div className='details-card'>\n              <div className='logo-container'>\n                <div className='logo-spot'>\n                  <Eyeglasses />\n                </div>\n              </div>\n              <h5>Built to Fit You</h5>\n              <p>Select your frames, and we will ensure the lenses meet your exact\n                prescription needs.\n              </p>\n            </div>\n          </div>\n        </div>\n\n      </div>\n\n    </div>\n  );\n};\n\nexport default Homepage;","/home/brandon/the-odin-project/javascript-path/projects/shopping-site-demo/src/ItemPage.js",["66","67"],"/home/brandon/the-odin-project/javascript-path/projects/shopping-site-demo/src/CartPage.js",["68","69","70","71","72"],"import React, { useState } from 'react';\nimport Item from './Item';\n\nconst CartPage = (props) => {\n  const { cartArr, addToCart, importAllImages, editQtyInCart } = props;\n  const [subTotal, setSubTotal] = useState(0);\n  const images = importAllImages(require.context('./assets/images', false, /\\.(png|jpe?g|svg|webp)$/));\n\n  const addZeroes = (num) => {\n    const dec = num.split('.')[1];\n    const len = dec && dec.length > 2 ? dec.length : 2;\n    return Number(num).toFixed(len);\n  }\n\n  const handleChange = (item, e) => {\n    console.log(item)\n    console.log(e.target.value)\n\n    \n    editQtyInCart(item, e.target.value);\n\n    //element will update because useState is being called, find a way to work it\n    console.log(e.target.value * item.price)\n    let product = (e.target.value * item.price)\n    setSubTotal(e.target.value * item.price);\n    console.log(subTotal)\n  };\n\n  const subtotalCalc = (item) => {\n    let prod = item.qty * item.price;\n    return prod\n  }\n\n  return (\n    <div className='checkout-cart-container'>\n      <div className='checkout-cart'>\n        <h1>Cart</h1>\n\n        <table>\n          <thead>\n            <tr>\n              <th scope='col'>Eyeglasses</th>\n              <th scope='col'>Model</th>\n              <th scope='col'>Price</th>\n              <th scope='col'>Quantity</th>\n              <th scope='col'>Subtotal</th>\n            </tr>\n          </thead>\n          <tbody>\n            {cartArr.map((item) => {\n              return (\n                <tr key={item.title} className='cart-item'>\n                  <th className='item-img'>\n                    <img src={images[item.imgId + '.webp'].default} ></img>\n                  </th>\n                  <td className='item-title'>{item.title} {item.subtitle}</td>\n                  <td className='item-price'>${item.price}</td>\n                  <td className='item-qty'>\n                    <input type='number' className='btn-counter' min='1' max='99' onChange={handleChange.bind(this, item)} defaultValue={item.qty} />\n                  </td>\n                  <td className='item-subtotal'>${subtotalCalc(item)}</td>\n                </tr>\n              )\n            })}\n          </tbody>\n        </table>\n\n        <div className='checkout-total'>\n          <h4>Cart Total</h4>\n          <div className='total-figure'>\n            <p>Total</p>\n            <p>${cartArr.reduce((a,b) => a + (b.qty * b.price), 0)}</p>\n          </div>\n          <hr></hr>\n          <button>Proceed To Checkout</button>\n        </div>\n\n      </div>\n    </div>\n  )\n}\n\nexport default CartPage;","/home/brandon/the-odin-project/javascript-path/projects/shopping-site-demo/src/ItemsContainer.js",[],"/home/brandon/the-odin-project/javascript-path/projects/shopping-site-demo/src/ShoppingCart.js",["73","74"],"import React, { useState } from 'react';\nimport { Link } from 'react-router-dom';\n\nconst ShoppingCart = (props) => {\n  const {cart} = props;\n  const [counts, setCounts] = useState({});\n\n  return (\n    <div className='cart'>\n      <Link to='/cartpage'>\n        View Cart\n      </Link>\n      {cart.map((item) => (\n        <div key={item.title}>{item.title} - Qty: {item.qty}</div>\n      ))}\n    </div>\n  )\n}\n\nexport default ShoppingCart;","/home/brandon/the-odin-project/javascript-path/projects/shopping-site-demo/src/RelatedProducts.js",["75","76","77","78"],"/home/brandon/the-odin-project/javascript-path/projects/shopping-site-demo/src/Item.js",["79","80","81","82"],"import React, { useState } from 'react';\n\nconst Item = (props) => {\n  const { item, addToCart, importAllImages } = props;\n\n  const itemStyle = {\n    border: item.borderStyle,\n  };\n\n  const images = importAllImages(require.context('./assets/images', false, /\\.(png|jpe?g|svg|webp)$/));\n\n  return (\n    <div className='itemShoppingPage-container'>\n      <div className='itemShoppingPage'>\n\n        <div className='img-container'>\n          <img className='itemShoppingPage-img' src={images[item.imgId + '.webp'].default} ></img>\n        </div>\n\n        <div className='info-container'>\n          <h3 className='item-title'>{item.title}</h3>\n          <h4 className='item-subtitle'>{item.subtitle}</h4>\n          <span className='item-price'>${item.price}</span>\n        </div>\n\n\n      </div> \n    </div>\n  );\n};\n\nexport default Item;",{"ruleId":"83","severity":1,"message":"84","line":4,"column":8,"nodeType":"85","messageId":"86","endLine":4,"endColumn":14},{"ruleId":"87","replacedBy":"88"},{"ruleId":"89","replacedBy":"90"},{"ruleId":"83","severity":1,"message":"91","line":3,"column":10,"nodeType":"85","messageId":"86","endLine":3,"endColumn":20},{"ruleId":"83","severity":1,"message":"92","line":4,"column":8,"nodeType":"85","messageId":"86","endLine":4,"endColumn":11},{"ruleId":"93","severity":1,"message":"94","line":26,"column":32,"nodeType":"95","messageId":"96","endLine":26,"endColumn":34},{"ruleId":"83","severity":1,"message":"97","line":1,"column":17,"nodeType":"85","messageId":"86","endLine":1,"endColumn":25},{"ruleId":"83","severity":1,"message":"98","line":3,"column":8,"nodeType":"85","messageId":"86","endLine":3,"endColumn":20},{"ruleId":"83","severity":1