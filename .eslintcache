 {\n              return (\n                <tr key={item.title} className='cart-item'>\n                  <th className='item-img'>\n                    <img src={images[item.imgId + '.webp'].default} ></img>\n                  </th>\n                  <td className='item-title'>{item.title} {item.subtitle}</td>\n                  <td className='item-price'>${item.price}</td>\n                  <td className='item-qty'>\n                    <input type='number' className='btn-counter' min='1' max='99' onChange={handleChange.bind(this, item)} defaultValue={item.qty} />\n                  </td>\n                  <td className='item-subtotal'>${subtotalCalc(item)}</td>\n                </tr>\n              )\n            })}\n          </tbody>\n        </table>\n\n        <div className='checkout-total'>\n          <h4>Cart Total</h4>\n          <div className='total-figure'>\n            <p>Total</p>\n            <p>${cartArr.reduce((a,b) => a + (b.qty * b.price), 0)}</p>\n          </div>\n          <hr></hr>\n          <button>Proceed To Checkout</button>\n        </div>\n\n      </div>\n    </div>\n  )\n}\n\nexport default CartPage;","/home/brandon/the-odin-project/javascript-path/projects/shopping-cart/src/ItemPage.js",["83","84"],"/home/brandon/the-odin-project/javascript-path/projects/shopping-cart/src/Images.js",[],["85","86"],"/home/brandon/the-odin-project/javascript-path/projects/shopping-cart/src/RelatedProducts.js",["87","88","89"],"import React, { useState } from 'react';\nimport { Link } from 'react-router-dom';\nimport Catalog from './Catalog.json';\n\nconst RelatedProducts = (props) => {\n  const { importAllImages, currentItem, dismissNotification } = props\n  const images = importAllImages(require.context('./assets/images', false, /\\.(png|jpe?g|svg|webp)$/));\n  let items = [];\n\n  while (items.length < 4) {\n    let itemId = Math.floor(Math.random() * Catalog.length) + 1\n    let item = Catalog.find((item) => item.id == itemId);\n    if (items.i